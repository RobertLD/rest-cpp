{
    "version": 3,
    "configurePresets": [
        {
            "name": "dev-perf",
            "displayName": "Dev (tests+perf)",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build/dev-perf",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo",
                "REST_CPP_BUILD_TESTS": "ON",
                "REST_CPP_BUILD_PERF": "ON"
            }
        }
    ]
}